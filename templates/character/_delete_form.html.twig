<form method="post" action="{{ path('character_delete', {'id': character.id}) }}" onsubmit="return confirm('Voulez-vous supprimer ce personnage ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ character.id) }}">
    <button class="btn btn-danger"><span uk-icon="trash"></span></button>
</form>
